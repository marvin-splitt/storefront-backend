{
    "spec_dir": "src",
    "spec_files": [
        "models/tests/*[sS]pec.ts",
        "routes/tests/*[sS]pec.ts"
    ],
    "helpers": ["helpers/**/*.js"],
    "reporters": [
        {
            "name": "jasmine-spec-reporter#SpecReporter",
            "options": {
                "displayStacktrace": "all"
            }
        }
    ],
    "stopSpecOnExpectationFailure": false,
    "random": true
}
